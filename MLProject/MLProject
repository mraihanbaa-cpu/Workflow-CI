
name: BookPopularityClassification

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 20}
      csv_path: {type: string, default: "top_1000_books_preprocessing.csv"}
    command: "python modelling.py {n_estimators} {max_depth} {csv_path}"
